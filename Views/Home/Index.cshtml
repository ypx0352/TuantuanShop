                                    </div>
                                    <div class="p-2">
                                        @if (firstHotSaleProduct.OnSale)
                                        {
                                            <span class="badge bg-warning" style="font-size:xx-small">On Sale</span>
                                            <span class="badge bg-danger" style="font-size: xx-small">Hot Sale</span>
                                            <span class='badge bg-success @(firstHotSaleProduct.InStock? "":"d-none")' style="font-size: xx-small">In Stock</span>
                                            <h5 class="mb-2 " style="line-height:2rem; max-height:4rem; overflow:hidden; text-overflow:ellipsis">@firstHotSaleProduct.Name</h5>
                                            <div>
                                                <span class="text-primary">￥ @firstHotSaleProduct.OnSalePrice </span>
                                                <span class="text-decoration-line-through" style="font-size:x-small"> ￥ @firstHotSaleProduct.Price</span>
                                            </div>
                                        }
                                        else
                                        {
                                            <span class='badge bg-success @(firstHotSaleProduct.InStock? "":"d-none")' style="font-size: xx-small">In Stock</span>
                                            <span class="badge bg-danger " style="font-size: xx-small">Hot Sale</span>
                                            <h5 class="mb-2 text-wrap" style="line-height:2rem; max-height:4rem; overflow:hidden; ">@firstHotSaleProduct.Name</h5>
                                            <span class="text-primary">￥ @firstHotSaleProduct.Price </span>
                                        }
                                    </div>
                                </div>
                            </div>
                        </div>
                    }

                    @*Right side list*@
                    <div class="col">
                        <div id="carouselHotSaleIndicators" class="carousel slide carousel-dark h-100" data-bs-ride="carousel">
                            <div class="carousel-inner h-100 position-relative">
                                <div class="carousel-indicators position-absolute top-0" style="height:24px">
                                    @for (int i = 0; i < (int)Math.Ceiling((double)(hotSaleProducts.Count() - 1) / 3); i++)
                                    {
                                        <button type="button" data-bs-target="#carouselHotSaleIndicators" data-bs-slide-to=@i class='@(i == 0? "active":"")' aria-current="true" aria-label="Slide @i"></button>
                                    }
                                </div>
                                @for (int i = 1; i < hotSaleProducts.Count(); i += 3)
                                {
                                    List<ProductForListViewModel> productsForOneCarouselSlide = new List<ProductForListViewModel>();
                                    @for (int j = 0; j < 3; j++)
                                    {
                                        if (newArrivalProducts.ElementAtOrDefault(i + j) != null)
                                        {
                                            productsForOneCarouselSlide.Add(hotSaleProducts.ElementAtOrDefault(i + j));
                                        }
                                    }

                                    @*Each carousel slide*@
                                    <div class="carousel-item @(i == 1? "active": "") position-absolute" style="top:30px">
                                        <div class="row row-cols-1 gy-0">
                                            @foreach (var product in productsForOneCarouselSlide)
                                            {
                                                if (product != null)
                                                {
                                                    @*Each product*@
                                                    <div class="col" role="button" onclick="location.href='@Url.Action("Show","Product",new{id=product.Id})'">
                                                        <div style="max-height:100px">
                                                            <div class="row row-cols-2 gy-1 my-1 bg-light">
                                                                <div class="col-4">
                                                                    <div class="text-center ">
                                                                        <img class="img-fluid" src="@product.ProfilePictureUrl" alt="@product.Name">
                                                                    </div>
                                                                </div>
                                                                <div class="col-8">
                                                                    <p class="text-wrap" style="overflow:hidden;  line-height: 1rem; max-height: 2rem">@product.Name</p>
                                                                    <p class="text-primary" style="padding-right:1px">￥ @(product.OnSale? product.OnSalePrice : product.Price)</p>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>
                                                }
                                            }
                                        </div>
                                    </div>
                                }
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

