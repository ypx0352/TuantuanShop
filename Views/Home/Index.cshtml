                {
                    <div class="col">
                        <div class="card h-100 w-100" role="button" onclick="location.href='@(Url.Action("Show","Product",new{id=product.Id}))'">
                            <div class="text-center">
                                <img class="card-img-top img-adjusted" src="@product.ProfilePictureUrl" alt="@product.Name" />
                            </div>
                            <div class="p-2">
                                @if (product.OnSale)
                                {
                                    <span class="badge bg-warning" style="font-size:xx-small">On Sale</span>
                                    <span class="badge bg-danger @(product.HotSale? "":"d-none")" style="font-size: xx-small">Hot Sale</span>
                                    <span class='badge bg-success @(product.InStock? "":"d-none")' style="font-size: xx-small">In Stock</span>
                                    <p class="mb-2 " style="line-height:1rem; max-height:2rem; overflow:hidden; text-overflow:ellipsis">@product.Name</p>
                                    <div>
                                        <span class="text-primary">￥ @product.OnSalePrice </span>
                                        <span class="text-decoration-line-through" style="font-size:x-small"> ￥ @product.Price</span>
                                    </div>
                                }
                                else
                                {
                                    <span class='badge bg-success @(product.InStock? "":"d-none")' style="font-size: xx-small">In Stock</span>
                                    <span class="badge bg-danger @(product.HotSale? "":"d-none")" style="font-size: xx-small">Hot Sale</span>
                                    <p class="mb-2 text-wrap" style="line-height:1rem; max-height:2rem; overflow:hidden; ">@product.Name</p>
                                    <span class="text-primary">￥ @product.Price </span>
                                }
                            </div>
                        </div>
                    </div>
                }
            </div>
        </div>
    }
</div>



<partial name="_BottomNavigationBarPartial" />
